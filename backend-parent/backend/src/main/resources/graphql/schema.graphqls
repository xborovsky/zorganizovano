type ContactQueryType {
    id: ID!
    type: String!
    sortKey: Int
}

type BlogPost {
    id: ID!
    published: String!
    title: String!
    contentPreview: String!
    content: String!
    titlePhoto: String!
    linkHref: String
    linkContent: String
    metaTitle: String
}

type ServerNotification {
    id: ID!
    text: String!
}

type Query {
    contactQueryTypes: [ContactQueryType]! 
    blogPosts: [BlogPost]!
    serverNotification: ServerNotification
}